<% @house.errors.full_messages.each do |message| %>
  <p>
  <%= message %>
  </p>
<% end %>

<%= form_for @house do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :rulers_of_the%>
  <%= f.text_field :rulers_of_the%>

  <%= f.label :photo_url%>
  <%= f.text_field :photo_url%>

  <%= f.submit %>
<% end %>
